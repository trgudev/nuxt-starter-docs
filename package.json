    "@nuxt/content": "^3.7.1",
    "@nuxt/image": "^1.11.0",
    "@nuxt/ui": "^4.0.1",
    "better-sqlite3": "^12.4.1",
    "nuxt": "^4.1.3",
    "nuxt-llms": "0.1.3",
    "nuxt-og-image": "^5.1.12"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@soybeanjs/cli": "^1.4.0",
    "eslint": "^9.38.0",
    "lint-staged": "^16.2.6",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.1.1"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm soy git-commit-verify",
